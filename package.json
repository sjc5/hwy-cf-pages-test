{
  "name": "cf-pages-tester",
  "private": true,
  "type": "module",
  "scripts": {
    "hwy-prebuild": "tailwindcss -i src/styles/tw-input.css -o src/styles/tw-output.bundle.css",
    "build": "tsc --noEmit && hwy-build",
    "dev:serve": "hwy-dev-serve",
    "dev:wrangler": "wrangler pages dev ./dist --compatibility-flag=\"nodejs_compat\" --port=5555 --live-reload",
    "dev": "npm run build && npm-run-all --parallel dev:*"
  },
  "dependencies": {
    "@hono/node-server": "^1.2.0",
    "hono": "^3.7.5",
    "hwy": "^0.4.0-beta.29"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231002.0",
    "@hwy-js/build": "^0.4.0-beta.29",
    "@hwy-js/dev": "^0.4.0-beta.29",
    "@types/node": "^20.8.3",
    "htmx.org": "^1.9.6",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "wrangler": "^3.11.0"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "prettier": {}
}
